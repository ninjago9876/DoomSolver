{
    "compilerOptions": {
      "types": ["@cloudflare/workers-types"],
      "target": "es2022",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["es2022", "WebWorker"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "outDir": "dist"
    },
    "include": ["src"]
}